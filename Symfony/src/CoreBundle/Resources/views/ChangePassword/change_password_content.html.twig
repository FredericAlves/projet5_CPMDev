{% trans_default_domain 'FOSUserBundle' %}
<link href="{{ asset('bundles/core/css/adminIndex.css') }}" rel="stylesheet">

<div id="changePassword">
    <h1 id="changePasswordTitle">Changez votre mot de passe</h1>
    {{ form_start(form, { 'action': path('fos_user_change_password'), 'attr': { 'class': 'fos_user_change_password' } }) }}
        {{ form_widget(form) }}
        <div id="changePasswordBtn">
            <input class="btn btn-default" type="submit" value="{{ 'change_password.submit'|trans }}" />
        </div>
    {{ form_end(form) }}
</div>